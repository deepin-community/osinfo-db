  <os id="http://freedos.org/freedos/1.3">
    <short-id>freedos1.3</short-id>
    <name>FreeDOS 1.3</name>
    <version>1.3</version>
    <vendor>FreeDOS</vendor>
    <family>dos</family>
    <distro>freedos</distro>
    <derives-from id="http://freedos.org/freedos/1.2"/>

    <release-date>2022-02-20</release-date>

    <variant id="standard">
      <name>FreeDOS 1.3 "Standard" Installer</name>
    </variant>
    <variant id="legacy">
      <name>FreeDOS 1.3 "Legacy" Installer</name>
    </variant>

    <media arch="i686" live="true">
      <variant id="standard"/>
      <iso>
        <volume-id>FD13-LiveCD</volume-id>
        <system-id>LINUX</system-id>
      </iso>
    </media>

    <media arch="i686">
      <variant id="legacy"/>
      <iso>
        <volume-id>FD13-LEGACY</volume-id>
        <system-id>LINUX</system-id>
      </iso>
    </media>

    <resources arch="all">
      <minimum>
        <n-cpus>1</n-cpus>
        <cpu>1000000000</cpu>
        <ram>33554432</ram>
        <storage>209715200</storage>
      </minimum>

      <recommended>
        <ram>67108864</ram>
        <storage>419430400</storage>
      </recommended>
    </resources>
  </os>
</libosinfo>
