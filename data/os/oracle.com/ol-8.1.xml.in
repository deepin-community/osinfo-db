<?xml version="1.0" encoding="UTF-8"?>
<libosinfo version="0.0.1">
  <os id="http://oracle.com/ol/8.1">
    <short-id>ol8.1</short-id>
    <name>Oracle Linux 8.1</name>
    <version>8.1</version>
    <vendor>Oracle America</vendor>
    <family>linux</family>
    <distro>ol</distro>
    <clones id="http://redhat.com/rhel/8.1"/>

    <release-date>2019-11-15</release-date>

    <media arch="x86_64">
      <url>https://yum.oracle.com/ISOS/OracleLinux/OL8/u1/x86_64/OracleLinux-R8-U1-x86_64-dvd.iso</url>
      <iso>
        <system-id>LINUX</system-id>
        <volume-id>OL-8-1-(\d-)?.*x86_64</volume-id>
      </iso>
      <kernel>isolinux/vmlinuz</kernel>
      <initrd>isolinux/initrd.img</initrd>
