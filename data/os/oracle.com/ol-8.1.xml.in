      <kernel>images/pxeboot/vmlinuz</kernel>
      <initrd>images/pxeboot/initrd.img</initrd>
    </media>

    <tree arch="x86_64">
      <treeinfo>
        <family>Oracle Linux</family>
        <version>^8.1(.([0-9]|[1-9][0-9]))?$</version>
        <arch>x86_64</arch>
      </treeinfo>
    </tree>
    <tree arch="aarch64">
      <treeinfo>
        <family>Oracle Linux</family>
        <version>^8.1(.([0-9]|[1-9][0-9]))?$</version>
        <arch>aarch64</arch>
      </treeinfo>
    </tree>

  </os>
</libosinfo>
